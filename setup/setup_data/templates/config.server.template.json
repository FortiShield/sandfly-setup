{
	"config_version": 2,
	"server": {
		"keys": {
			"server": {
				"public_key": "server_keys_server_public_key_b64",
				"private_key": "server_keys_server_private_key_b64"
			},
			"node": {
				"public_key": "server_keys_node_public_key_b64"
			},
			"jwt": "server_keys_jwt"
		},
		"ssl": {
			"server": {
				"cacert": "server_ssl_server_cacert_b64",
				"cert": "server_ssl_server_cert_b64",
				"cert_signed": "server_ssl_server_cert_signed_b64",
				"private_key": "server_ssl_server_key_b64",
				"private_key_signed": "server_ssl_server_key_signed_b64"
			},
			"node": {
				"cert": "server_ssl_node_cert_b64",
				"private_key": "server_ssl_node_key_b64"
			}
		},
		"rabbit": {
			"hostname": "sandfly-rabbit",
			"node_password": "rabbit_node_password"
		},
		"api": {
			"hostname": "api_hostname",
			"password": "api_password"
		},
		"db": {
			"postgres": {
				"connstr": "db_postgres_connstr",
				"pool_size": 50,
				"postgres_password": "db_postgres_postgres_password",
				"sandfly_password": "db_postgres_sandfly_password"
			}
		},
		"options": {
			"log_level": "info",
			"disable_strict_tls_policy": false
		}
	}
}

